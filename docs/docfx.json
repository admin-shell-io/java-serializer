{
    "build": {
        "content": [
            {
                "files": [
                    "articles/**.md",
                    "**/*.yml",
                    "*.md",
                    "toc.yml"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadata": {
            "_appTitle": "java-serializer Documentation",
            "_appFaviconPath": "images/favicon.ico",
            "_appLogoPath": "images/adminshellio-48x48.jpg",
            "_enableSearch": true
        },
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}